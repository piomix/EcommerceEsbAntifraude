		<IdTransaccion>$!body.datosCompra.idTransaccion</IdTransaccion>
	</TransactionDetails>
	<PaymentDetails>
		<PaymentCurrencyType>$!body.datosCompra.tipoMoneda</PaymentCurrencyType>
		<Total>$!body.datosCompra.montoTotal</Total>
		<AdditionalCharges>$!body.datosCompra.additionalCharges</AdditionalCharges>		 
	</PaymentDetails>
	<CreditCard>
		<Number>$!body.datosTarjeta.numTarjeta</Number>
		<ExpirationMonth>$!body.datosTarjeta.mesVencimiento</ExpirationMonth>
		<ExpirationYear>$!body.datosTarjeta.anioVencimiento</ExpirationYear>
		<Installment>$!body.datosCompra.cuotas</Installment>
		<BIN>$!body.datosTarjeta.bin</BIN>
		<IssuingBank>$!body.datosTarjeta.issuingBank</IssuingBank>
