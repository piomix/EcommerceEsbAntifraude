<AntiFraudBrokerMessage>
	<Header>
		<BusinessLine>$!body.encabezado.lineaNegocio</BusinessLine>
		<Channel>$!body.encabezado.canal</Channel>
		<Commerce>$!body.encabezado.commerce</Commerce>
		<RiskValidationService>$!body.encabezado.riskValidationService</RiskValidationService>
		<PhoneCallerID>$!body.encabezado.phoneCallerID</PhoneCallerID>
		<Reference>$!body.encabezado.reference</Reference>
	</Header>
	<TransactionDetails>
		<PNR>$!body.datosVuelo.pnr</PNR>
		<ReserveOffice>$!body.datosVuelo.oficinaReserva</ReserveOffice>
		<TransactionSourceCountry>$!body.datosCompra.pais</TransactionSourceCountry>
		<GapRegistrationTCandBuy>$!body.datosCompra.gapRegistrationTCandBuy</GapRegistrationTCandBuy>
		<Agent>$!body.datosCompra.agent</Agent>
		<DescriptionAncillary>$!body.datosCompra.descriptionAncillary</DescriptionAncillary>
		<ContainsAncillary>$!body.datosCompra.containsAncillary</ContainsAncillary>
