	</CreditCard>
	<CreditCardHolder>
		<BillingStreet>$!body.datosTarjeta.direccionTH</BillingStreet>
		<BillingStreet2>$!body.datosTarjeta.direccion2TH</BillingStreet2>
		<BillingCity>$!body.datosTarjeta.ciudadTH</BillingCity>
		<BillingState>$!body.datosTarjeta.estadoTH</BillingState>
		<BillingCountry>$!body.datosTarjeta.paisTH</BillingCountry>
		<FirstName>$!body.datosTarjeta.nombreTH</FirstName>
		<LastName>$!body.datosTarjeta.apellidoTH</LastName>
		<Email>$!body.datosCliente.email</Email>
		<ZipCode>$!body.datosTarjeta.codPostalTH</ZipCode>
		<DocumentType>$!body.datosTarjeta.tipoDocumentoTH</DocumentType>
		<DocumentNumber>$!body.datosTarjeta.numDocumentoTH</DocumentNumber>
		<BankRegisteredPhoneNumber>$!body.datosTarjeta.telefonoTH</BankRegisteredPhoneNumber>
	</CreditCardHolder>
	<Client>
		<AlternativePhone>$!body.datosCliente.telAlterno</AlternativePhone>
		<Gender>$!body.datosCliente.sexo</Gender>
		<BirthDate>$!headers.fecNac</BirthDate>
		<FrequentFlyerNumber>$!body.viajeroFrecuente.codLifemiles</FrequentFlyerNumber>
		<ProfileInscriptionDate>$!headers.fecIns</ProfileInscriptionDate>
	</Client>
	<Flight>
		<Type>$!body.datosVuelo.tipoViaje</Type>
		<Path>$!body.datosVuelo.path</Path>
		<PassengerCount>$!body.datosVuelo.passengerCount</PassengerCount>
		<From>$!body.datosVuelo.from</From>
		<To>$!body.datosVuelo.to</To>
		<ChangeDate>$!headers.fecChange</ChangeDate>
		<Duration>$!body.datosVuelo.duration</Duration>
		<AirlineCode>$!body.datosVuelo.airlinecode</AirlineCode>
		<FareBasis>$!body.datosVuelo.fareBasis</FareBasis>
		<DayOfWeek>$!body.datosVuelo.dayOfWeek</DayOfWeek>
		<WeekOfYear>$!body.datosVuelo.weekOfYear</WeekOfYear>
	</Flight>
