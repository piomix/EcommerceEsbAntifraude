<AntiFraudBrokerMessage>
	<Header>
		<BusinessLine>$!body.encabezado.lineaNegocio</BusinessLine>
		<IPAddress>$!body.encabezado.ip</IPAddress>
		<Channel>$!body.encabezado.canal</Channel>
		<Devicefingerprint>$!body.encabezado.deviceFingerprint</Devicefingerprint>
	</Header>
	<TransactionDetails>
		<PNR>$!body.datosVuelo.pnr</PNR>
		<ReserveOffice>$!body.datosVuelo.oficinaReserva</ReserveOffice>
		<TransactionSourceCountry>$!body.datosCompra.pais</TransactionSourceCountry>
		<EventDate>$!body.datosCompra.eventDate</EventDate>
		<IdTransaccion>$!body.datosCompra.idTransaccion</IdTransaccion>
	</TransactionDetails>
	<PaymentDetails>
		<PaymentCurrencyType>$!body.datosCompra.tipoMoneda</PaymentCurrencyType>
