		<FirstName>$!body.datosTarjeta.nombreTH</FirstName>
		<LastName>$!body.datosTarjeta.apellidoTH</LastName>
		<Email>$!body.datosCliente.email</Email>
		<ZipCode>$!body.datosTarjeta.codPostalTH</ZipCode>
		<DocumentType>$!body.datosTarjeta.tipoDocumentoTH</DocumentType>
		<DocumentNumber>$!body.datosTarjeta.numDocumentoTH</DocumentNumber>
		<BankRegisteredPhoneNumber>$!body.datosTarjeta.telefonoTH</BankRegisteredPhoneNumber>
	</CreditCardHolder>
	<Client>
		<AlternativePhone>$!body.datosCliente.telAlterno</AlternativePhone>
		<Gender>$!body.datosCliente.sexo</Gender>
		<BirthDate>$!headers.fecNac</BirthDate>
		<FrequentFlyerNumber>$!body.viajeroFrecuente.codLifemiles</FrequentFlyerNumber>
		<FrequentFlyerCategory>$!body.viajeroFrecuente.categoria</FrequentFlyerCategory>
		<MilesAccumulated>$!body.viajeroFrecuente.millasAcumuladas</MilesAccumulated>
		<BalanceMiles>$!body.viajeroFrecuente.saldoMillas</BalanceMiles>
		<FrequentFlyerAgentCreator>$!body.viajeroFrecuente.viajeroFrecuenteCreado</FrequentFlyerAgentCreator>
		<FrequentFlyerDocument>$!body.viajeroFrecuente.docViajeroFrecuente</FrequentFlyerDocument>
		<TransactionTypeMiles>$!body.viajeroFrecuente.tipoTransaccionLM</TransactionTypeMiles>
		<QuantityMiles>$!body.viajeroFrecuente.cantidadMillas</QuantityMiles>
		<FrequentFlyerFullName>$!body.datosCliente.nombre</FrequentFlyerFullName>
		<FrequentFlyerFullLastName>$!body.datosCliente.apellido</FrequentFlyerFullLastName>
		<ProfileInscriptionDate>$!headers.fecIns</ProfileInscriptionDate>
	</Client>
	<Flight>
		<Type>$!body.datosVuelo.tipoViaje</Type>
	</Flight>
